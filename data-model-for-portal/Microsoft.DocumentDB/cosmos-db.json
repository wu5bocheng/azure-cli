{
    "resourceType": "cosmos-db",
    "apiVersion": "2022-11-15",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/cosmosdb"
    },
    "commands": [
        {
            "name": "az cosmosdb list",
            "description": "List Azure Cosmos DB database accounts.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DocumentDB/databaseAccounts",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cosmosdb#az-cosmosdb-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List all Azure Cosmos DB database accounts in a resource group.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az cosmosdb show",
            "description": "Get the details of an Azure Cosmos DB database account.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DocumentDB/databaseAccounts/{accountName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cosmosdb#az-cosmosdb-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--ids]",
                            "[--name]",
                            "[--resource-group]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Get the details of an Azure Cosmos DB database account. (autogenerated)",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.accountName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az cosmosdb database list",
            "description": "Lists all Azure Cosmos DB databases.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DocumentDB/databaseAccounts/{accountName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cosmosdb/database#az-cosmosdb-database-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--key]",
                            "[--name]",
                            "[--resource-group-name]",
                            "[--url-connection]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Lists all Azure Cosmos DB databases.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.accountName]"
                        },
                        {
                            "name": "--resource-group-name",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az cosmosdb network-rule list",
            "description": "Lists the virtual network accounts associated with a Cosmos DB account.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DocumentDB/databaseAccounts/{accountName}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cosmosdb/network-rule#az-cosmosdb-network-rule-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "--name",
                            "--resource-group",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Lists the virtual network accounts associated with a Cosmos DB account.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.accountName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az cosmosdb keys list",
            "description": "List the access keys or connection strings for a Azure Cosmos DB database account.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DocumentDB/databaseAccounts/{accountName}/listKeys",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cosmosdb/keys#az-cosmosdb-keys-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "--name",
                            "--resource-group",
                            "[--type]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "List the access keys or connection strings for a Azure Cosmos DB database account.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.accountName]"
                        },
                        {
                            "name": "--resource-group-name",
                            "value": "[path.resourceGroupName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az cosmosdb collection list",
            "description": "Lists all Azure Cosmos DB collections.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DocumentDB/databaseAccounts/{accountName}/databases/{databaseRid}/collections",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cosmosdb/collection#az-cosmosdb-collection-list"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "--db-name",
                            "[--key]",
                            "[--name]",
                            "[--resource-group-name]",
                            "[--url-connection]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Lists all Azure Cosmos DB collections.",
                    "parameters": [
                        {
                            "name": "--db-name",
                            "value": "[path.databaseRid]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.accountName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az cosmosdb collection show",
            "description": "Shows an Azure Cosmos DB collection and its offer.",
            "path": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.DocumentDB/databaseAccounts/{accountName}/databases/{databaseRid}/collections/{collectionRid}",
            "confirmation": false,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/cosmosdb/collection#az-cosmosdb-collection-show"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "--collection-name",
                            "--db-name",
                            "[--key]",
                            "[--name]",
                            "[--resource-group-name]",
                            "[--url-connection]",
                            "[--subscription]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Shows an Azure Cosmos DB collection and its offer.",
                    "parameters": [
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--collection-name",
                            "value": "[path.collectionRid]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--db-name",
                            "value": "[path.databaseRid]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.accountName]"
                        }
                    ]
                }
            ]
        }
    ]
}
