                    }
                ]
            },
            "examples": [
                {
                    "description": "Get details of a security partner provider.",
                    "parameters": [
                        {
                            "name": "--name",
                            "value": "[path.securityPartnerProviderName]"
                        },
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        }
                    ]
                }
            ]
        }
    ]
