{
    "resourceType": "workspaces/tables",
    "apiVersion": "2021-12-01-preview",
    "learnMore": {
        "url": "https://docs.microsoft.com/cli/azure/monitor/log-analytics/workspace/table"
    },
    "commands": [
        {
            "name": "az monitor log-analytics workspace table delete",
            "description": "Delete a Log Analytics workspace table.",
            "path": "/subscriptions/{subscriptionId}/resourcegroups/{resourceGroupName}/providers/Microsoft.OperationalInsights/workspaces/{workspaceName}/tables/{tableName}",
            "confirmation": true,
            "help": {
                "learnMore": {
                    "url": "https://docs.microsoft.com/cli/azure/monitor/log-analytics/workspace/table#az-monitor-log-analytics-workspace-table-delete"
                },
                "parameterSets": [
                    {
                        "parameters": [
                            "[--resource-group]",
                            "[--subscription]",
                            "[--name]",
                            "[--table-name]",
                            "[--workspace-name]"
                        ]
                    }
                ]
            },
            "examples": [
                {
                    "description": "Delete a Log Analytics workspace table.",
                    "parameters": [
                        {
                            "name": "--resource-group",
                            "value": "[path.resourceGroupName]"
                        },
                        {
                            "name": "--subscription",
                            "value": "[path.subscriptionId]"
                        },
                        {
                            "name": "--name",
                            "value": "[path.tableName]"
                        },
                        {
                            "name": "--workspace-name",
                            "value": "[path.workspaceName]"
                        }
                    ]
                }
            ]
        },
        {
            "name": "az monitor log-analytics workspace table migrate",
            "description": "Migrate a Log Analytics table from support of the Data Collector API and Custom Fields features to support of Data Collection Rule-based Custom Logs.",
